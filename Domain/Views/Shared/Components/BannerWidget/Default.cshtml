@model IEnumerable<BannersListVM>

@if (Model.Count() > 0)
{

    @foreach (var item in Model)
    {
        if (!string.IsNullOrEmpty(item.MainImageFileId))
        {
            <a href="@Html.Raw(item.Link)" title="@item.Label" target="_blank">
                <img src="@Html.Raw(Url.Action("ViewImage", "FileCdn", new { area = "", id = item.MainImageFileId }))" />
            </a>
        }
    }
}
