@model IEnumerable<ArticleListVM>
@inject IStringLocalizer<CommonResources> Localizer
@if (Model.Count() > 0)
{
    @foreach (var article in Model)
    {
        <div class="fixed-item">
            <a asp-action="View" asp-controller="Publication" asp-route-id="@article.Id">
                <p class="description">
                    @*<b>@article.EventDate.ToString(GlobalConstants.DateFormat)</b>*@
                    @Html.Raw(article.Title)
                </p>
            </a>
        </div>
    }
}
else
{
    <span>Няма данни.</span>
}