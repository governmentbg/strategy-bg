@{
    ViewBag.PageTitle = PopForums.Resources.AddForum;
    Layout = "~/Areas/Forums/Views/Admin/AdminMaster.cshtml";
}

<h2>@PopForums.Resources.AddForum</h2>

<form method="post">
	<div class="form-horizontal" role="form">
		@await Html.PartialAsync("ForumForm")
		<div class="form-group">
			<div class="col-sm-push-2 col-sm-10">
				<input type="submit" value="@PopForums.Resources.AddForum" class="btn btn-primary" />
			</div>
		</div>
	</div>
</form>