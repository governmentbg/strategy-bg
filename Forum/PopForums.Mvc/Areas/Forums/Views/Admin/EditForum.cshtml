@model Forum

@{
    ViewBag.PageTitle = PopForums.Resources.EditForum;
    Layout = "~/Areas/Forums/Views/Admin/AdminMaster.cshtml";
}

<h2>@PopForums.Resources.EditForum</h2>

<form method="post">
	<div class="form-horizontal" role="form">
		@await Html.PartialAsync("ForumForm", Model)
		<div class="form-group">
			<div class="col-sm-push-2 col-sm-10">
				<input type="submit" value="@PopForums.Resources.Save" class="btn btn-primary" />
			</div>
		</div>
	</div>
</form>